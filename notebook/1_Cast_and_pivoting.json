							"deleting": false
						}
					}
				},
				"source": [
					"#1 - importing col function \r\n",
					"\r\n",
					"from pyspark.sql.functions import col\r\n",
					"\r\n",
					"df.filter(col('Unemployed')== 2000).show(n=4)"
				],
				"execution_count": 7
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
