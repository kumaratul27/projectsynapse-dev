					}
				},
				"source": [
					"#2 - Importing entire function module as using alias as F \r\n",
					"\r\n",
					"from pyspark.sql import functions as F\r\n",
					"\r\n",
					"df.filter(F.col('Unemployed')== 2000).show()"
