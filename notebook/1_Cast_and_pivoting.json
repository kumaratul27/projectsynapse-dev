				"source": [
					"from pyspark.sql import functions as F\r\n",
					"\r\n",
					"df_Casted = df.withColumn('Unemployed',F.col('Unemployed').cast('float'))"
				],
				"execution_count": 14
			},
			{
				"cell_type": "code",
				"metadata": {
