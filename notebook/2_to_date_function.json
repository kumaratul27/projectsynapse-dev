				"source": [
					"df.select('Date_Inserted').show()\r\n",
					"\r\n",
					"# MM/dd/YYYY"
				],
				"execution_count": 5
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
