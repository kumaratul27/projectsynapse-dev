				"source": [
					"## - Accessing the Files from Mountpoint \r\n",
					"## Syntax:\r\n",
					"# synfs:/<jobid>/<mountpoint>/<path>\r\n",
					"# To get JobID - mssparkutils.env.getJobId()\r\n",
					"\r\n",
					"\r\n",
					"job_id = mssparkutils.env.getJobId()\r\n",
					"\r\n",
					"mount_point = 'synfs:/' + job_id + '/lake'"
				],
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
